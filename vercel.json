{
    "version": 2,
    "builds": [
        
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "npm run build"
            }
        },
        {
            "src": "prisma/schema.prisma",
            "use": "prisma"
          }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}